<!DOCTYPE html>
<html>

<head>
  <meta charset="utf-8">
  <title>
    <%= title %>
  </title>
</head>

<body>
  <h1><%= title %></h1>

  <table>
    <thead>
      <tr>
        <% for (let key in result[0]) { %>
        <th>
          <%= key %>
        </th>
        <% } %>
      </tr>
    </thead>
    <tbody>
      <% // for (let object of result ) {} %>
      <% for (let i=0, len=result.length; i<len; i+=1) { %>
      <tr>
        <% for (let key in result[i]) { %>
        <td>
          <%= result[i][key] %>
        </td>
        <% } %>
      </tr>
      <% } %>
    </tbody>
  </table>

</body>

</html>